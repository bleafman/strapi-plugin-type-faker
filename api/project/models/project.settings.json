{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "demoUrl": {
      "type": "string"
    },
    "codeUrl": {
      "type": "string"
    },
    "technologies": {
      "collection": "technologies",
      "via": "projects"
    }
  }
}
